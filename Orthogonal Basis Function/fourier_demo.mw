<Group hide-output="false" labelreference="L8900" drawlabel="true">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">TrigonometricApproximation := proc( m::integer, n::integer, f::procedure, verbose:=true )

  local t, x, a, b, approx;


  t := [ seq( 2*Pi*i/m, i=0..(m-1) ) ]:
  x := f~(t):

  # print( 't[k]' = t );
  # print( 'x[k]' = x );

  a := unapply( 2/m * add( x[i] * cos( k*t[i] ), i=1..m ), k ):
  b := unapply( 2/m * add( x[i] * sin( k*t[i] ), i=1..m ), k ):


  if verbose then
    print( 'a[j]' = [ seq( a(j), j=0..n ) ] );
    print( 'b[j]' = [ seq( b(j), j=0..n ) ] );
  end:

  if ( m = 2*n ) then
     approx := a(0)/2 + sum( a(k)*cos(k*'t'), k=1..(n-1) ) + a(n)/2*cos(n*'t') + sum( b(k)*sin(k*'t'), k=1..(n-1) );
  else
     approx := a(0)/2 + sum( a(k)*cos(k*'t'), k=1..n ) + sum( b(k)*sin(k*'t'), k=1..n );
  end if:

  return unapply( approx, 't' ):

end proc:
</Text-field>
</Input>
</Group>
<Group hide-output="false" labelreference="L8906" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">TrigonometricInterpolation :=( m::integer, f::procedure ) -&gt; TrigonometricApproximation( m, floor(m/2), f ):
</Text-field>
</Input>
</Group>
